{
    "name": "prueba-tecnica",
    "version": "1.0.0",
    "main": "index.js",
    "author": "Santiago Muñoz Castañeda",
    "license": "ISC",
    "description": "Construcción de CRUD con PHP y AngularJS - Boostrap",
    "dependencies": {
      "angular": "1.8.x",
      "angular-route": "^1.8.3",
      "bootstrap": "3.3.x"
    },
    "devDependencies": {
      "concurrently": "^9.0.1",
      "http-server": "^0.11.1"
    },
    "scripts": {
      "dev": "concurrently \"php -S localhost:3600 -t backend\" \"http-server ./app -a localhost -p 8000 -c-1\"",
      "copy-libs": "cpx \"node_modules/{angular,angular-*,bootstrap/dist}/**/*\" app/lib -C",
      "prestart": "npm install",
      "start": "http-server ./app -a localhost -p 8000 -c-1",
      "pretest": "npm install",
      "test": "karma start karma.conf.js",
      "test-single-run": "npm test -- --single-run",
      "preupdate-webdriver": "npm install",
      "update-webdriver": "webdriver-manager update",
      "preprotractor": "npm run update-webdriver",
      "protractor": "protractor e2e-tests/protractor.conf.js",
      "php-server": "php -S localhost:3600 -t backend"
    }
  }